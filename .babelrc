{
  "presets": [
    "react",
    "flow",
    ["env", {
      "targets": {
        "browsers": [
          "ie >= 11",
          "last 1 Edge version",
          "last 1 Firefox version",
          "last 1 Chrome version",
          "last 1 Safari version",
          "last 1 iOS version",
          "last 1 Android version",
          "last 1 ChromeAndroid version"
        ]
      }
    }]
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-export-extensions"
  ],
  "comments": false
}